    "test:homePage:all": "cypress run --browser chrome --spec 'cypress/e2e/HomePage/**/*'",
    "test:subPages:all": "cypress run --browser chrome --spec 'cypress/e2e/SubPages/**/*'",
    "test:subPages:features": "cypress run --browser chrome --spec 'cypress/e2e/SubPages/FeaturesPage/*'",
    "test:subPages:knowledge": "cypress run --browser chrome --spec 'cypress/e2e/SubPages/KnowledgebasePage/*'",
    "test:subPages:privacy": "cypress run --browser chrome --spec 'cypress/e2e/SubPages/PrivacyPolicyPage/*'",
    "test:subPages:terms": "cypress run --browser chrome --spec 'cypress/e2e/SubPages/TermsOfServicePage/*'"
